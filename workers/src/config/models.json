    "@cf/black-forest-labs/flux-1-schnell": {
      "id": "@cf/black-forest-labs/flux-1-schnell",
      "name": "FLUX.1 [schnell]",
      "description": "Fast 12B parameter rectified flow transformer for rapid image generation",
      "provider": "black-forest-labs",
      "apiVersion": 2,
      "inputFormat": "json",
      "responseFormat": "base64",
      "supportedTasks": [
        "text-to-image"
      ],
      "parameters": {
        "prompt": {
          "cfParam": "prompt",
          "type": "string",
          "required": true,
          "description": "Text description of the image"
        },
        "steps": {
          "cfParam": "steps",
          "type": "integer",
          "default": 4,
          "min": 1,
          "max": 8,
          "description": "Diffusion steps (higher = better quality, slower)"
        },
        "seed": {
          "cfParam": "seed",
          "type": "integer",
          "default": null,
          "description": "Random seed for reproducibility"
        }
      },
      "limits": {
        "maxPromptLength": 2048,
        "defaultSteps": 4,
        "maxSteps": 8,
        "minWidth": 512,
        "maxWidth": 2048,
        "minHeight": 512,
        "maxHeight": 2048,
        "supportedSizes": [
          "512x512",
          "768x768",
          "1024x1024"
        ]
      }
    },
