{
  "name": "@cloudflare-image-mcp/local",
  "version": "0.1.0",
  "description": "Local deployment of Cloudflare Image MCP with REST API, MCP server, and web UI",
  "author": "tan-yong-sheng",
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "cloudflare-image-local": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/main.ts",
    "start": "node --env-file=.env dist/main.js"
  },
  "keywords": [
    "cloudflare",
    "ai",
    "image-generation",
    "mcp",
    "local"
  ],
  "dependencies": {
    "@cloudflare-image-mcp/core": "file:../core",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
