{
  "name": "cloudflare-image-mcp",
  "version": "0.0.6",
  "description": "Cloudflare Workers AI Image Generator MCP Server",
  "author": "tan-yong-sheng",
  "author_url": "https://github.com/tan-yong-sheng",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cloudflare-image-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "cloudflare",
    "ai",
    "image-generation",
    "workers-ai",
    "flux",
    "stable-diffusion",
    "leonardo-ai"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.54.0",
    "tsx": "^4.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tan-yong-sheng/cloudflare-image-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/tan-yong-sheng/cloudflare-image-mcp/issues"
  },
  "homepage": "https://github.com/tan-yong-sheng/cloudflare-image-mcp#readme"
}
